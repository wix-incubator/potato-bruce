{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --devtool false",
    "start": "webpack-dev-server --open",
    "serve": "serve ./dist -s",
    "lint": "eslint . --ext .js,.ts,.tsx -f codeframe",
    "typecheck": "tsc --noEmit",
    "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^3.4.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@stylable/core": "^3.4.5",
    "@stylable/webpack-plugin": "^3.4.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
