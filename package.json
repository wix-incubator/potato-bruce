{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack --mode production --devtool false",
    "start": "webpack-dev-server --open",
    "serve": "serve ./dist -s",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "yarn typecheck && yarn lint",
    "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write",
    "predeploy": "yarn build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^3.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@stylable/core": "^3.9.0",
    "@stylable/webpack-plugin": "^3.9.0",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.3",
    "typescript": "~4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
