{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --progress --open",
    "serve": "serve ./dist -s",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "yarn typecheck && yarn lint",
    "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write",
    "predeploy": "yarn build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^3.11.13",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@stylable/core": "^3.11.13",
    "@stylable/webpack-plugin": "^3.11.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.14",
    "typescript": "~4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
