{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack --mode production --devtool false",
    "start": "webpack-dev-server --open",
    "serve": "serve ./dist -s",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "yarn typecheck && yarn lint",
    "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write",
    "predeploy": "yarn build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^3.11.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@stylable/core": "^3.11.5",
    "@stylable/webpack-plugin": "^3.11.5",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.1.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.7",
    "typescript": "~4.0.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
