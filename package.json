{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --progress --open",
    "st:watch": "stc --cjs false --dts true -w",
    "serve": "serve ./dist -s",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "yarn typecheck && yarn lint",
    "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write",
    "predeploy": "yarn build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^4.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@stylable/cli": "^4.3.3",
    "@stylable/core": "^4.3.3",
    "@stylable/webpack-plugin": "^4.3.3",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "~4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3"
  }
}
