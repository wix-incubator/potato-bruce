{
  "name": "potato-bruce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --progress --open",
    "st:watch": "stc --cjs false --dts true -w",
    "serve": "serve ./dist -s",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && npm run lint && npm run typecheck",
    "prettify": "npx prettier . --write",
    "predeploy": "npm run build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "@stylable/runtime": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stylable/cli": "^5.2.0",
    "@stylable/core": "^5.2.0",
    "@stylable/webpack-plugin": "^5.2.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "~4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  }
}
